* 
	margin: 0

.container
	margin: 0 auto
	max-width: 1500px

.logo
	position: absolute
	left: 0
	right: 0
	top: 0
	bottom: 0
	margin: auto
#home
	height: 100%
	width: 100%
	position: fixed
	background-position: center


#home:before 
	content: " "
	width: 100%
	height: 100%
	position: absolute
	z-index: -1
	top: 0
	left: 0
	background: -webkit-radial-gradient(top center, ellipse cover, rgba(255,255,255,0.2) 0%,rgba(0,0,0,.5) 70%)



